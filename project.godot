; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Asteroid"
run/main_scene="res://Scenes/Main_Menu.tscn"
config/icon="res://icon.png"

[display]

window/size/width=720
window/size/height=480
window/stretch/mode="2d"
window/stretch/aspect="keep"

[layer_names]

2d_render/layer_1="Comet"
2d_physics/layer_1="Comet"
2d_render/layer_2="SpaceShip"
2d_physics/layer_2="SpaceShip"
2d_render/layer_3="Bullet"
2d_physics/layer_3="Bullet"

[logging]

file_logging/enable_file_logging=true

[rendering]

environment/default_clear_color=Color( 0.12549, 0.117647, 0.117647, 1 )
environment/default_environment="res://default_env.tres"
